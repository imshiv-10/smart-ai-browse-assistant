# Smart Browse Assistant Backend Configuration

# App settings
DEBUG=false
APP_NAME="Smart Browse Assistant"

# CORS (comma-separated origins or ["*"] for all)
CORS_ORIGINS=["*"]

# LLM Configuration
# Options: ollama, vllm, openai
LLM_PROVIDER=ollama
LLM_MODEL=qwen2.5:7b
LLM_BASE_URL=http://localhost:11434
LLM_API_KEY=  # Required for OpenAI/Anthropic
LLM_TEMPERATURE=0.7
LLM_MAX_TOKENS=4096

# Scraper Configuration
SCRAPER_HEADLESS=true
SCRAPER_TIMEOUT=30000
SCRAPER_MAX_PAGES=5

# Cache Configuration
CACHE_ENABLED=true
CACHE_TTL=3600
